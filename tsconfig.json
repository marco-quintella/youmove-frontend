{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "vueCompilerOptions": {
    "plugins": [
      "@volar/vue-language-plugin-pug"
    ]
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "app/*": [
        "*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "boot/*": [
        "src/boot/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "composables/*": [
        "src/composables/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "services/*": [
        "src/services/*"
      ],
      "services": [
        "src/services/index.ts"
      ],
      "src/*": [
        "src/*"
      ],
      "stores/*": [
        "src/stores/*"
      ],
      "stores": [
        "src/stores/index.ts"
      ],
      "types/*": [
        "src/types/*"
      ],
      "types": [
        "src/types/index.d.ts"
      ],
      "utils/*": [
        "src/utils/*"
      ],
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/auto-imports.d.ts",
    "src/components.d.ts"
  ]
}
